<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Fable.OidcClient.Cybernara</PackageId>
    <Version>1.0.3</Version>
    <Authors>Maciej Jarosiewicz; Kayden Bui</Authors>
    <Company>Cybernara</Company>
    <PackageDescription>Fable binding for oidc-client javascript library</PackageDescription>
    <PackageTags>fsharp;fable;oidc;oidc-client;openid-connect</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/dneivzz/Fable.OidcClient</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Fable.OidcClient.fs" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="*.fsproj; *.fs; *.js" PackagePath="fable\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Fable.Core" Version="3.7.1" />
  </ItemGroup>
  <PropertyGroup>
    <NpmDependencies>
      <NpmPackage Name="oidc-client" Version="gt 1.11.0 lt 2.0.0" ResolutionStrategy="Max" />
    </NpmDependencies>
</PropertyGroup>
</Project>
